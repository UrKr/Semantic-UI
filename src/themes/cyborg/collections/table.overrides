table,
.table {
    color: #fff;

    a:not(.button) {
        color: #fff;
        text-decoration: underline;
    }

    .dropdown a {
        text-decoration: none;
    }

    &.stackable {
        background-color: darken(@gray-darker, 4%);
    }
}


.ui.table thead:not(.full-width) th:first-child {
    box-shadow: none !important;
}
