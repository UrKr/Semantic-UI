.ui.table {

  .positive,
  .warning,
  .negative,
  .error {
    color: #fff;
  }

  &.celled tbody {

    tr.positive,
    tr.warning,
    tr.negative {

      td,
      &:hover td {
        border-color: darken(@gray-darker, 5%);
      }
    }
}

.ui.table.stackable {
  background-color: @lighten(@pageBackground, 3%);
}
